{
  "name": "react-native-firebase-perf-tracer",
  "version": "0.1.0",
  "description": "React Native Component Performance Tracer based on Firebase Performance",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "author": "zenklub <open@zenklub.com.br>",
  "homepage": "https://github.com/zenklub/react-native-firebase-perf-tracer#readme",
  "repository": "git+https://github.com/zenklub/react-native-firebase-perf-tracer.git",
  "bugs": {
    "url": "https://github.com/zenklub/react-native-firebase-perf-tracer/issues"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "tslint -c ./tslint.json ./src/**/*.{ts,tsx}",
    "lint:fix": "tslint -c ./tslint.json ./src/**/*.{ts,tsx} --fix",
    "test": "jest",
    "build": "yarn lint && tsc",
    "prepare": "npm run build",
    "prepublish": "npm run build"
  },
  "peerDependencies": {
    "@react-native-firebase/app": "^6.3.4",
    "@react-native-firebase/perf": "^6.3.4",
    "react": "^16.13.0",
    "react-native": "^0.61.5"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-decorators": "^7.6.2",
    "@eva-design/eva": "^2.0.0",
    "@eva-design/material": "^2.0.0",
    "@react-native-firebase/perf": "^7.1.6",
    "@types/jest": "ts3.8",
    "@types/react-native": "ts3.8",
    "@types/react-test-renderer": "ts3.8",
    "@types/rimraf": "ts3.8",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "coveralls": "^3.0.2",
    "husky": "^1.1.2",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react": "~16.9.0",
    "react-native": "~0.61.4",
    "react-native-testing-library": "^1.12.0",
    "react-test-renderer": "16.9.0",
    "rimraf": "^2.6.2",
    "ts-node": "^3.2.2",
    "tslint": "^5.12.1",
    "typescript": "^3.8.2"
  },
  "resolutions": {
    "react": "~16.9.0",
    "react-native": "~0.61.5",
    "@react-native-firebase/perf": "~6.3.4"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
